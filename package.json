{
  "name": "codex-linux",
  "version": "1.0.3",
  "description": "A Linux port of OpenAI Codex - Multi-agent AI coding command center",
  "main": "dist/main/main.js",
  "author": "Codex Linux Team",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json --skipLibCheck --noEmit false || true",
    "build:renderer": "vite build",
    "package": "electron-builder",
    "package:linux": "electron-builder --linux",
    "package:appimage": "electron-builder --linux AppImage",
    "package:deb": "electron-builder --linux deb",
    "package:rpm": "electron-builder --linux rpm",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "cd tests/e2e && npx playwright test",
    "test:e2e:ui": "cd tests/e2e && npx playwright test --ui",
    "test:e2e:report": "cd tests/e2e && npx playwright show-report",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc -p tsconfig.main.json --noEmit",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@monaco-editor/react": "^4.6.0",
    "@octokit/rest": "^20.0.2",
    "@slack/web-api": "^7.14.1",
    "@supabase/supabase-js": "^2.97.0",
    "axios": "^1.6.2",
    "better-sqlite3": "^9.4.0",
    "chokidar": "^3.5.3",
    "conf": "^10.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.6",
    "date-fns": "^3.0.6",
    "diff": "^5.1.0",
    "electron-log": "^5.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "fuse.js": "^7.0.0",
    "glob": "^10.3.10",
    "helmet": "^8.1.0",
    "ignore": "^5.3.0",
    "isomorphic-git": "^1.25.6",
    "lodash": "^4.17.21",
    "lucide-react": "^0.303.0",
    "monaco-editor": "^0.55.1",
    "mongodb": "^7.1.0",
    "mysql2": "^3.17.5",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "openai": "^4.28.0",
    "pg": "^8.18.0",
    "playwright": "^1.58.2",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "react-virtualized": "^9.22.5",
    "semver": "^7.5.4",
    "simple-git": "^3.22.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/diff": "^5.0.9",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/prismjs": "^1.26.3",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-virtualized": "^9.21.29",
    "@types/semver": "^7.5.6",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "build": {
    "appId": "com.codex-linux.app",
    "productName": "Codex Linux",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "assets/**/*",
      "node_modules/**/*"
    ],
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "Development",
      "icon": "assets/icon.png",
      "maintainer": "Codex Linux Team",
      "vendor": "Codex Linux Team",
      "synopsis": "Multi-agent AI coding command center",
      "description": "A Linux port of OpenAI Codex featuring parallel agent orchestration, worktrees, skills, and automations"
    },
    "deb": {
      "depends": [
        "git",
        "nodejs"
      ]
    },
    "rpm": {
      "depends": [
        "git",
        "nodejs"
      ]
    }
  }
}
